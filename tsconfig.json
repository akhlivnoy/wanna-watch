{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "#assets": ["./app/assets"],
      "#assets/*": ["./app/assets/*"],
      "#components": ["./app/components"],
      "#components/*": ["./app/components/*"],
      "#constants": ["./app/constants"],
      "#constants/*": ["./app/constants/*"],
      "#hooks": ["./app/hooks"],
      "#hooks/*": ["./app/hooks/*"],
      "#models": ["./app/models"],
      "#models/*": ["./app/models/*"],
      "#navigation": ["./app/navigation"],
      "#navigation/*": ["./app/navigation/*"],
      "#redux": ["./app/redux"],
      "#redux/*": ["./app/redux/*"],
      "#screens": ["./app/screens"],
      "#screens/*": ["./app/screens/*"],
      "#services": ["./app/services"],
      "#services/*": ["./app/services/*"],
      "#themes": ["./app/themes"],
      "#themes/*": ["./app/themes/*"],
      "#types": ["./app/types"],
      "#types/*": ["./app/types/*"],
      "#utils": ["./app/utils"],
      "#utils/*": ["./app/utils/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
